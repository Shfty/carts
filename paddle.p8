-- entity
entity = {
	x = 0,
	y = 0,
	w = 0,
	h = 0,
	vx = 0,
	vy = 0
}

function entity:new(o)
	self.__index = self
	
	return setmetatable(
		o or {},
		self
	)
end

function entity:halfw()
	return self.w * 0.5
end

function entity:halfh()
	return self.h * 0.5
end

function entity.overlap(a, b)
	local dist_x
	dist_x = abs(a.x - b.x)
	dist_x *= 2
	
	local dist_y
	dist_y = abs(a.y - b.y)
	dist_y *= 2
	
	local size_x
	size_x = a.w + b.w
	
	local size_y
	size_y = a.h + b.h
	
 return dist_x < size_x and
        dist_y < size_y
end

function entity:_update()
	self.x += self.vx
	self.y += self.vy
end

function round(i)
	if(i % 1 >= 0.5)
	then
		return ceil(i)
	else
		return flr(i)
	end
end

