-- hud
function draw_hud()
	cursor(8,8)
	rectfill(6,6,64,20,0)
 color(7)
	print("bounces " .. bounces)
	
	if(bounces >= 30)
	then
		print("ssstop playing")
	elseif(bounces >= 20)
	then
		print("sssavage combo")
	elseif(bounces >= 15)
	then
		print("awesome combo")
	elseif(bounces >= 10)
	then
		print("badass combo")
	elseif(bounces >= 5)
	then
		print("cool combo")
	elseif(bounces >= 2)
	then
		print("dull combo")
	else
		print("no combo")
	end
end
