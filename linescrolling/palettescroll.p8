-- main

update_palfield_sync = false
update_palfield_async = false

scrollx = 0
scrolly = 0

function _init()
	if update_palfield_sync then
		gen_palfield()
	end
	
	if update_palfield_async then
		cstart_palfield()
	end
end

function _update60()
	ctick_palfield()
	
	if btn(0) then
		scrollx -= 1
	end
	
	if btn(1) then
		scrollx += 1
	end
	
	if btn(2) then
		scrolly -= 0.5
	end
	
	if btn(3) then
		scrolly += 0.5
	end
	
	if btnp(4) then
		vis_palfield =
			not vis_palfield
	end
	
	if btnp(5) then
		store_palfield()
	end

	scrolly -= 1
end

function _draw()
	cls()
	
	draw_palfield(
		64,
		scrollx,
		scrolly
	)
	
	draw_perfhud()
end

